<div class="block">
  <div class="secondary-navigation">
    <ul>
      <li class="first"><%= link_to 'List', schedules_path  %></li>
      <li><%= link_to 'New', new_schedule_path %></li>
      <li class="active"><%= link_to 'Edit', edit_schedule_path %></li>
    </ul>
    <div class="clear"></div>
  </div>          
  <div class="content">            
    <h2 class="title">Edit Schedule</h2>
    <div class="inner">
      <% form_for @schedule, :url => schedule_path(@schedule), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar', :locals => { :help => "Please enter the new scheduling information." }) -%>