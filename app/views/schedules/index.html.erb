<h1>Schedule Log</h1>
<table>
  <tr>
    <th>Start time</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Exposure</th>
    <th>Number of pictures</th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%=h schedule.start_time %></td>
    <td><%=h schedule.latitude %></td>
    <td><%=h schedule.longitude %></td>
    <td><%=h schedule.exposure %></td>
    <td><%=h schedule.number_of_pictures %></td>
    <td><%= link_to 'Show', schedule %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Delete', schedule, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New schedule', new_schedule_path %>