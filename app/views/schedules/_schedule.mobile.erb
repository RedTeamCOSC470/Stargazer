<tr class="<%= cycle("odd", "even") %>">
  <td>
		<%= schedule.start_time.strftime("%B %d, %Y") %>
  </td>
  <td>
		<%= link_to schedule.start_time.strftime("%I:%M:%S %p"), schedule_path(schedule) %>
  </td>
  <td class="last">
    <%= link_to 'show', schedule_path(schedule) %>
  </td>
</tr> 